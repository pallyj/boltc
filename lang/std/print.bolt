import intrinsics

public func printInt8(i: i8) 
public func printUInt8(i: i8)

public func printInt16(i: i16)
public func printUInt16(i: i16)

public func printInt32(i: i32)
public func printUInt32(i: i32)

public func printInt64(i: i64)
public func printUInt64(i: i64)

public func printDouble(n: f64)
public func printFloat(n: f32)

public func printBool(n: i1)

public func print(b: Bool) {
	printBool(b.repr)
}

public func print(f: Half) {
	printFloat( float16Ext32(f.repr) )
}

public func print(f: Float) {
	printFloat( f.repr )
}

public func print(f: Double) {
	printDouble( f.repr )
}

public func print(i: Int) {
	printInt64(i.repr)
}

public func print(i: Int64) {
	printInt64(i.repr)
}

public func print(i: Int32) {
	printInt32(i.repr)
}
public func print(i: Int16) {
	printInt16(i.repr)
}

public func print(i: Int8) {
	printInt8(i.repr)
}

public func print(i: UInt) {
	printUInt64(i.repr)
}

public func print(i: UInt64) {
	printUInt64(i.repr)
}

public func print(i: UInt32) {
	printUInt32(i.repr)
}
public func print(i: UInt16) {
	printUInt16(i.repr)
}

public func print(i: UInt8) {
	printUInt8(i.repr)
}
import intrinsics

/* testing line comments */
public struct Int {
	var repr: i64

	public func add(b: Int): Int {
		Int(integer64Add(self.repr, b.repr))
	}

	public func neq(b: Int): i1 {
		integer64CmpNeq(self.repr, b.repr)
	}

	public func print() {
		printInt(repr)
	}
}


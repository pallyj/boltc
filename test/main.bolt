func gcd(a: Int, b: Int): Int {
	if a.lt(b) {
		gcd(b, a)
	} else if a.mod(b).eq(0) {
		b
	} else {
		gcd(b, a.mod(b))
	}
}

func factorial(n: Int): Int {
	if n.lt(2) {
		1
	} else {
		n.mul(factorial(n.sub(1)))
	}
}

func main() {
	/*gcd(1, 1).print()

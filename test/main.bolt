func gcd(a: Int, b: Int): Int {
	if a.lt(b).repr {
		gcd(b, a)
	} else if a.mod(b).eq(0).repr {
		b
	} else {
		gcd(b, a.mod(b))
	}
}

func factorial(n: Int): Int {
	if n.lt(2).repr {
		1
	} else {
		n.mul(factorial(n.sub(1)))
	}
}


@entryPoint
func main() {
	for(0, 1_000_000, loopBody)
}

func factorial(n: Int): Int {
	if n.lt(2).repr {
		1
	} else {
		n.mul(factorial(n.sub(1)))
	}
}

func loopBody(i: Int) {
	factorial(i.mod(20)).print()
}

func for(low: Int, high: Int, f: func (Int)) {
	if low.neq(high).repr {
		f(low);
		for(low.add(1), high, f)
	}
}